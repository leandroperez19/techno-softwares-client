{
    "name": "techno-softwares-client",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc -b && vite build",
        "lint": "eslint .",
        "preview": "vite preview",
        "cypress:component": "npx cypress run --component",
        "dev-exposed": "vite --host",
        "docker:build-dev": "docker build -t techno_client_test:dev -f Dockerfile --target dev .",
        "docker:run-dev": "docker-compose up",
        "docker:build-prod": "npm run build && docker build -t techno_client_test:prod -f Dockerfile --target prod .",
        "docker:run-prod": "docker run -p 8080:8080 techno_client_test:prod"
    },
    "dependencies": {
        "@hookform/resolvers": "^3.9.1",
        "@tanstack/react-table": "^8.20.5",
        "axios": "^1.7.7",
        "bignumber.js": "^9.1.2",
        "d3": "^7.9.0",
        "dotenv": "^16.4.5",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-hook-form": "^7.53.2",
        "react-icons": "^5.3.0",
        "react-query": "^3.39.3",
        "react-router-dom": "^6.28.0",
        "react-toastify": "^10.0.6",
        "styled-components": "^6.1.13",
        "swiper": "^11.1.15",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@eslint/js": "^9.13.0",
        "@types/d3": "^7.4.3",
        "@types/node": "^22.9.1",
        "@types/react": "^18.3.12",
        "@types/react-dom": "^18.3.1",
        "@vitejs/plugin-react": "^4.3.3",
        "autoprefixer": "^10.4.20",
        "cypress": "^13.16.0",
        "eslint": "^9.13.0",
        "eslint-plugin-react-hooks": "^5.0.0",
        "eslint-plugin-react-refresh": "^0.4.14",
        "globals": "^15.11.0",
        "postcss": "^8.4.49",
        "tailwindcss": "^3.4.15",
        "typescript": "~5.6.2",
        "typescript-eslint": "^8.11.0",
        "vite": "^5.4.10",
        "vitest": "^2.1.5"
    }
}
